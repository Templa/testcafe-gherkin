{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "root": true,
  "extends": [
    "google"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "strict": "off",
    "no-console": "off",
    "import/no-unresolved": "off",
    "max-len": [0, 120],
    "new-cap": 0,
    "require-jsdoc": 0,
    "no-unused-vars": [1, { "vars": "all", "args": "none","varsIgnorePattern": "Given|When|Then", "ignoreRestSiblings": false }],
    "indent": ["error", 4]
  }
}
